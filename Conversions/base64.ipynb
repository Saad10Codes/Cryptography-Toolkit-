   "source": [
    "import base64\n",
    "\n",
    "# Example byte data\n",
    "data = b\"Hello, World!\"\n",
    "\n",
    "# Encode to base64\n",
    "encoded_data = base64.b64encode(data)\n",
    "print(f\"Base64 Encoded: {encoded_data}\")\n",
    "\n",
    "# Decode from base64\n",
    "decoded_data = base64.b64decode(encoded_data)\n",
    "print(f\"Decoded Bytes: {decoded_data}\")\n"
   ]
