   "source": [
    "def diffieHellman(primeNo,gen,ka,kb):\n",
    "    pka=(gen**ka)%primeNo\n",
    "    pkb=(gen**kb)%primeNo\n",
    "    ska=(pkb**ka)%primeNo\n",
    "    skb=(pka**kb)%primeNo\n",
    "    return (ska,skb)"
   ]
  },
